{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run main.py < input.txt",
      "type": "shell",
      "command": "${workspaceFolder}/.venv/bin/python ${workspaceFolder}/main.py < ${workspaceFolder}/input.txt",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
